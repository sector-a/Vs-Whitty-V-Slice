import funkin.play.song.Song;
import flixel.FlxG;

class RemorseSong extends Song {
    function new() {
        super('remorse');
    }

    override public function listDifficulties(variationId:String, variationIds:Array<String>, showLocked:Bool):Array<String> {
        if (showLocked || FlxG.save.data.isUpdikeUnlocked)
            return super.listDifficulties(variationId, variationIds);

        return [];
    }
}